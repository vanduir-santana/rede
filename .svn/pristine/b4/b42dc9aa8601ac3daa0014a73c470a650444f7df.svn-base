;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	youtaberai.com. root.youtaberai.com. (
			    139		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

; name server - NS records
@	IN	NS	youtaberai.com.

; PTR Records
13.22	IN	PTR	youtaberai.com.		; 172.16.13.22
13.22	IN	PTR	dbit.net.		    ; 172.16.13.22
13.22	IN	PTR	s.dbit.net.		    ; 172.16.13.22
22.0	IN	PTR	not.dbit.net.		; 172.16.0.22

; Clientes
