;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	youtaberai.com. root.yotuaberai.com. (
			    134		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

; name servers - NS records
@	IN	NS	youtaberai.com.
; name servers - A records
dbit.net.		IN	A	172.16.22.13
not.dbit.net.		IN	A	172.16.0.22

; Clientes
